module Blinker (
    led1: output bit,
    led2: output bit,
    led3: output bit,
    led4: output bit,
    led5: output bit,
    led6: output bit,
    s1  : input  bit,
    s2  : input  bit,
) {
    var led: bit<6>;
    assign {led6, led6, led4, led3, led2, led1} = led;
    // assign led1 = led[0];
    // assign led2 = led[1];
    // assign led3 = led[2];
    // assign led4 = led[3];
    // assign led5 = led[4];
    // assign led6 = led[5];

    let rst: reset_async_low = s1 & s2;

    var int_clock: clock;
    inst osc: $sv::Gowin_OSC (
        oscout: int_clock //output oscout
        ,    );

    always_ff (int_clock, rst) {
        if_reset {
            led = 1;
        } else {
            if led == 6'b100_000 {
                led = 1;
            } else {
                led = led << 1;
            }
        }
    }
}
